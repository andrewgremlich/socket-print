{
	"name": "socket-print",
	"productName": "Provel Print",
	"description": "A CAD-type application that facilitates a faster turnaround time for amputees to receive 3D printed socket for a residual limb.",
	"version": "1.0.0",
	"main": ".vite/build/main.js",
	"type": "module",
	"author": {
		"name": "andrewgremlich"
	},
	"license": "MIT",
	"repository": {
		"url": "https://github.com/andrewgremlich/socket-print"
	},
	"scripts": {
		"dev": "npm run dev:web",
		"dev:web": "vite --config vite.config.ts",
		"build": "vite build",
		"build:tauri": "tauri build",
		"preview": "vite preview --config vite.config.ts",
		"prepare": "husky",
		"test": "vitest run src/ --config ./vite.config.ts",
		"test:watch": "vitest src/ --config ./vite.config.ts",
		"test:coverage": "vitest run --coverage src/",
		"type-check": "tsc --noEmit",
		"check": "biome check --write ./src",
		"clean": "rimraf ./dist ./.vite ./out ./dev-dist",
		"tauri": "tauri"
	},
	"lint-staged": {
		"*.{js,ts,tsx}": [
			"npm run check"
		]
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@tauri-apps/cli": "^2.5.0",
		"@types/node": "^22.15.3",
		"@types/three": "^0.176.0",
		"@types/validator": "^13.15.0",
		"@vitest/coverage-v8": "^3.1.2",
		"browserslist": "^4.24.4",
		"happy-dom": "^17.4.7",
		"husky": "^9.1.7",
		"jsdom": "^26.1.0",
		"lightningcss": "^1.29.3",
		"lint-staged": "^15.5.1",
		"rimraf": "^6.0.1",
		"ts-node": "^10.9.2",
		"typescript": "^5.8.3",
		"vite": "^6.3.4",
		"vite-plugin-top-level-await": "^1.5.0",
		"vite-tsconfig-paths": "^5.1.4",
		"vitest": "^3.1.2"
	},
	"dependencies": {
		"@tauri-apps/api": "^2.5.0",
		"@tauri-apps/plugin-dialog": "^2.2.1",
		"@tauri-apps/plugin-fs": "^2.2.1",
		"@tauri-apps/plugin-log": "^2.4.0",
		"@tauri-apps/plugin-process": "^2.2.1",
		"@tauri-apps/plugin-updater": "^2.7.1",
		"crc-32": "^1.2.2",
		"dexie": "^4.0.11",
		"hotkeys-js": "^3.13.9",
		"mathjs": "^14.4.0",
		"p-throttle": "^7.0.0",
		"three": "^0.176.0",
		"three-mesh-bvh": "^0.9.0",
		"validator": "^13.15.0"
	}
}
