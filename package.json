{
	"name": "socket-print",
	"productName": "Provel Print",
	"description": "A CAD-type application that facilitates a faster turnaround time for amputees to receive 3D printed socket for a residual limb.",
	"version": "1.15.7",
	"type": "module",
	"author": {
		"name": "andrewgremlich"
	},
	"license": "MIT",
	"repository": {
		"url": "https://github.com/andrewgremlich/socket-print"
	},
	"scripts": {
		"dev": "npm run dev:web",
		"dev:web": "vite --config vite.config.ts",
		"build": "npm run clean && vite build",
		"build:tauri": "tauri build",
		"preview": "vite preview --config vite.config.ts",
		"prepare": "husky",
		"test": "vitest run src/ --config ./vite.config.ts",
		"test:watch": "vitest src/ --config ./vite.config.ts",
		"test:coverage": "vitest run --coverage src/",
		"check": "tsc --noEmit && biome check --write ./src",
		"clean": "rimraf ./dist ./.vite ./out ./dev-dist",
		"tauri": "tauri"
	},
	"lint-staged": {
		"*.{js,ts,tsx}": [
			"npm run check"
		]
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.11",
		"@tauri-apps/cli": "^2.9.6",
		"@types/node": "^24.10.8",
		"@types/three": "^0.182.0",
		"@types/validator": "^13.15.10",
		"@vitest/coverage-v8": "^4.0.17",
		"browserslist": "^4.28.1",
		"husky": "^9.1.7",
		"jsdom": "^27.4.0",
		"lightningcss": "^1.30.2",
		"lint-staged": "^16.2.7",
		"rimraf": "^6.1.2",
		"typescript": "^5.9.3",
		"vite": "^7.3.1",
		"vite-plugin-top-level-await": "^1.6.0",
		"vite-tsconfig-paths": "^6.0.4",
		"vitest": "^4.0.17"
	},
	"dependencies": {
		"@tauri-apps/api": "^2.9.1",
		"@tauri-apps/plugin-dialog": "^2.6.0",
		"@tauri-apps/plugin-fs": "^2.4.5",
		"@tauri-apps/plugin-log": "^2.8.0",
		"@tauri-apps/plugin-process": "^2.3.1",
		"@tauri-apps/plugin-updater": "^2.9.0",
		"crc-32": "^1.2.2",
		"dexie": "^4.2.1",
		"hotkeys-js": "^4.0.0",
		"lucide": "^0.562.0",
		"mathjs": "^15.1.0",
		"three": "^0.182.0",
		"three-mesh-bvh": "^0.9.6",
		"validator": "^13.15.26"
	}
}
